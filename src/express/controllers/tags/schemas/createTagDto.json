{
  "type": "object",
  "required": ["sportId", "title", "archived", "color", "icon", "kind", "private"],
  "additionalProperties": false,
  "properties": {
    "sportId": {
      "type": "string",
      "pattern": "^[a-fA-F0-9]{24}$"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "archived": {
      "type": "boolean"
    },
    "kind": {
      "enum": ["primary", "regular"]
    },
    "private": {
      "type": "boolean"
    },
    "color": {
      "type": "string"
    },
    "icon": {
      "type": "string"
    },
    "position": {
      "type": "number"
    }
  }
}
